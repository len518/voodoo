{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars":["error",{ "vars": "all", "args": "after-used", "ignoreRestSiblings": false, "argsIgnorePattern":"_*" } ]
        //"varsIgnorePattern":"[iI]gnored"
    }
}
